{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "WikiLearn"
   },
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "http://*.wikipedia.org/*", "https://*.wikipedia.org/*" ],
      "run_at": "document_start"
   } ],
   "description": "Learn while browsing Wikipedia",
   "homepage_url": "http://wikimine.org",
   "key": "",
   "manifest_version": 2,
   "name": "WikiMine",
   "options_page": "options.html",
   "permissions": [ "https://graph.facebook.com/*", "http://*.wikipedia.org/*", "https://*.wikipedia.org/*", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.8"
}
